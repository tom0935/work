@{
    ViewBag.Title = ViewData["NAME"];
}


<iframe id="iFrameContent" src="@ViewData["EXTRA"]" frameborder="0" scrolling="auto" seamless="seamless"> </iframe>



<script type="text/javascript">
    function chgFrame() {
        var w = $(window).width();
        $("#iFrameContent").width(w);

        var h = $(window).height();
        var menu = $("#NavMenu").height();
        $("#iFrameContent").height((h - menu)*0.9);    
    }

    $(document).ready(function () {
        chgFrame();
    });

    $(window).resize(function () {
        chgFrame();
    });
</script>